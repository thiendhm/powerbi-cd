	measure TopProduct_toBrand_Value = ```
			
			var _Rank = 
			    RANK(DENSE, ALLSELECTED(T_DimProduct[ProductName]), ORDERBY([SelectMetric], ASC))
			
			VAR _Result_Value = IF( _Rank <=1, [SelectMetric])
			RETURN _Result_Value
			```
		lineageTag: 43fde905-6f47-44ff-8702-a2aa3f5a0c54

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure TopBrand_Name = ```
			
			VAR AggTable = SUMMARIZE(T_FactSales, T_DimProduct[Brand],"Metric", [SelectMetric])
			VAR MaxValue =
			CALCULATE( 
			    MAXX( AggTable, [Metric]))
			VAR CategoryMax =  MAXX ( TOPN (1, AggTable, [Metric], DESC) , T_DimProduct[Brand])
			RETURN CategoryMax
			```
		lineageTag: 6e84dddb-37df-4ef1-abb0-07b31977fb44

