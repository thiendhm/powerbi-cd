	measure Total_Profit = SUM( T_FactSales[MarginEUR])
		lineageTag: 1c7ce112-9eca-4456-835b-9d8f232131c1

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Gross_Profit_Pct =
			
